pipeline {
    agent any

    stages {
        stage('Github') {
           steps {
               git branch: 'Chore/#8', url: 'https://github.com/EARLYBUDDY-TEAM/EB-Server.git'
           }
        }
        stage('Build & Deploy') {
            dir('api')
            steps {
                sh 'sudo docker-compose up -d'
            }
        }
    }
}

